generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["mongoDb"]
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model exercise {
  id     String @id @default(dbgenerated()) @map("_id") @db.ObjectId
  name   String
  userId String
}

model workout {
  id        String @id @default(dbgenerated()) @map("_id") @db.ObjectId
  createdAt String
  updatedAt String
  userId    String
}

model workoutset {
  id         String @id @default(dbgenerated()) @map("_id") @db.ObjectId
  amount     String
  createdAt  String
  exerciseId String
  unit       String
  updatedAt  String
  userId     String
  volume     String
  workoutId  String
}
