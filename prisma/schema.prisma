generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["mongoDb"]
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model exercise {
  id     String @id @default(dbgenerated()) @map("_id") @db.ObjectId
  name   String
  userId String
}

model workout {
  id        String   @id @default(dbgenerated()) @map("_id") @db.ObjectId
  createdAt DateTime @default(now())
  name      String
  updatedAt DateTime @updatedAt
  userId    String
}

model workoutset {
  id         String   @id @default(dbgenerated()) @map("_id") @db.ObjectId
  amount     String
  createdAt  DateTime @default(now())
  exerciseId String
  unit       String
  updatedAt  DateTime @updatedAt
  userId     String
  volume     String
  workoutId  String
}
